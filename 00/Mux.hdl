/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
	Xor(a=a, b=b, out=preIf);
	And(a=preIf, b=sel, out=If);
	Xor(a=a,b=If,out=out);
}
